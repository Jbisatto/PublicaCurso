package media;

import javax.swing.JOptionPane;

public class CalculoDeMedia {

	public static void main(String[] args) {
		int nota1 =JOptionPane.showConfirmDialog(null, "Informe a primeira nota:");
		int nota2 =JOptionPane.showConfirmDialog(null, "Informe a segunda nota:");
		int nota3 =JOptionPane.showConfirmDialog(null, "Informe a terceira nota:");
		double media=(nota1+nota2+nota3)/3;
		JOptionPane.showConfirmDialog(null, "A médiad é :"+media);
		

	}

}

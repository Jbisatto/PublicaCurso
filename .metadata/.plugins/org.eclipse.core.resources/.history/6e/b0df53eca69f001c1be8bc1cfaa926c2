package exercicio07;

public class Matriz25posicoes {

	public static void main(String[] args) {
		int [][] matriz = new int [5][5];
		int contador=1, soma=0;
		
//Preenchimeto da matriz		
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5; j++) {
				matriz[i][j] = contador;
				contador++;

			}

		}
	
//Exibir matriz
		for (int i = 0; i < 5; i++) {
			System.out.println("");
			for (int j = 0; j < 5; j++) {
				System.out.print(matriz[i][j] + "  ");
			}

		}
//Somar valores matriz		
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5; j++) {
				matriz[i][j] = contador;
				soma+=matriz[i][j];

			}

		}
		System.out.println("\nSoma Total:"+soma);
	}

}

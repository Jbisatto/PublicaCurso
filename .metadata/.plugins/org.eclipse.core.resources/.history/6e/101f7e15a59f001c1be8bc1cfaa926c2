package exercicio07;

public class Matriz25posicoes {

	public static void main(String[] args) {
		int [][] matriz = new int [5][5];
		int contador=1;
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5; j++) {
				matriz[i][j] = Integer.toString(contador).charAt(0);
				contador++;

			}

		}
	}

}
